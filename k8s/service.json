{
   "apiVersion":"v1",
   "kind":"Service",
   "metadata":{
      "namespace":"kube-system",
      "name":"${APP_NAME}-service",
      "labels":{
          "k8s-service":"${APP_NAME}"
      }
   },
   "spec":{
       "selector":{
           "k8s-app": "${APP_NAME}"
       },
       "ports":[
           {"port":8080,"targetPort":8080}
       ],
       "type":"NodePort"
   }
}
