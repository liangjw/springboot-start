{
   "apiVersion":"v1",
   "kind":"Service",
   "metadata":{
      "namespace":"kube-system",
      "name":"${JOB_NAME}-service",
      "labels":{
          "service":"${JOB_NAME}"
      }
   },
   "spec":{
       "selector":{
           "k8s-app": "${JOB_NAME}"
       },
       "ports":[
           {"port":"8080","targetPort":"8080"}
       ],
       "type":"NodePort"
   }
}
